#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int temp,rem,sum=0,n;
  
  printf("Enter a number:");
  scanf("%d",&n);
  temp=n;
  while(n>0){
    rem=n%10;
    sum=sum+rem*rem*rem;
    n=n/10;
    
  }
  if(temp==sum)
    
  
  printf("It is an Armstrong number");
  else
    
  
  printf("Not an Armstrong number");
  return 0;
}
