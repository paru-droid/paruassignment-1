#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int imatrix[10][10];
  int i,j,m,n;
  printf("Enter row:\n");
  scanf("%d",&m);
  
  printf("Enter coloumn:\n");
  scanf("%d",&n);
  for(i=0;i<m;i++){
    for(j=0;j<n;j++){
      if(i==j)
      {
        printf("1");
      }else{
        printf("0");
      }
    }
    
  
  printf("\n");
  }
return 0;
      
  
  
}
