#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int matrix[10][10];
  int i,j,m,n,e=0,o=0;
  printf("Enter order of matrix\n");
  scanf("%d%d",&m,&n);
  printf("Enter coefficients of matrix\n");
  for(i=0;i<m;i++)
  {
    for(j=0;j<n;j++)
    {  scanf("%d",&matrix[i][j]);
      if((matrix[i][j]%2)==0)
      {
        e++;
      }else{
        
      
      o++;}
    }
  }
  printf("The frequency of even elements=%d\n",e);
  printf("The frequency of odd elements =%d",o);
  return 0;
}
