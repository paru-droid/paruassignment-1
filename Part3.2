#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  float P,R,T,SI;
  printf("Enter first number:");
  scanf("%f",&P);
  printf("Enter second number:");
  scanf("%f",&R);
  printf("Enter third number:");
  scanf("%f",&T);
  /* Formula to calculate Simple Interest*/
  SI=(P*R*T)/100;
  printf("Answer:%f",SI);
  
  return 0;
}
