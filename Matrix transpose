include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  
  int i,j,m,n,a[10][10];
  printf("Enter order:\n");
  scanf("%d%d",&m,&n);
  printf("Enter elements...\n");
  for(i=0;i<m;i++)
 
   
  for(j=0;j<n;j++)
  scanf("%d",&a[i][j]);
  for(i=0;i<m;i++)
  for(j=0;j<n;j++)
  
    printf("%d",a[j][i]);
  
  printf("\n");
  return 0;
}
