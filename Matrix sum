#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  int matrix[10][10];
  int i,j,m,n,sum=0;
  printf("Enter order\n");
  scanf("%d%d",&m,&n);
  printf("Enter elements\n");
  for(i=0;i<m;i++){
    for(j=0;j<n;j++){
      scanf("%d",&matrix[i][j]);
    }
  }
  
  for(i=0;i<m;i++){
    for(j=0;j<n;j++){
      sum =sum + matrix[i][j];
    }
  
  printf("sum of %d row is=%d\n",i,sum);
  sum=0;
  } sum=0;
  for(j=0;j<n;j++){
    for(i=0;i<m;i++){
      sum =sum + matrix[i][j];
    }
  
  printf("sum of %d coloumn is=%d\n",j,sum);
  sum=0;
  }
  return 0;
}
