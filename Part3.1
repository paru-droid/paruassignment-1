#include<stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  char name[10];
  printf("Enter name of the student:");
  fgets(name,10,stdin);
  printf("Student Name is %s.\n",name);
  double mark1;
 
  double mark2;
  double mark3;
  double Average;
 
  printf("Enter first number:");
  scanf("%lf",&mark1);
  printf("Enter second number:");
  scanf("%lf",&mark2);
  printf("Enter third number:");
  scanf("%lf",&mark3);
 
  /* Formula of Average*/
  Average=(mark1+mark2+mark3)/3;
  /* Result given as*/
  printf("Answer:%f", Average);
  return 0;
}
