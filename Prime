#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
  
 int n,i,x=0; 
  printf("Enter a number");
  scanf ("%d",&n);
  for(i=2;i<=n-1;i++)
  {
    if(n%i==0)
    {
      x++;
    }
  }
    if(x==0)
    {
      printf("The given number is a prime number");
      }else{
        printf("Not a prime number");
      }
    
 return 0;
 
}
